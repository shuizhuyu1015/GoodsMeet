.lamp_container {
  background-color: #fe4655;
  /* position: fixed; */
  width: 100%;
  height: 50rpx;
  line-height: 44rpx;
  /* top: 0;
  left: 0;
  z-index: 10; */
}

.lamp_text {
  color: #fff;
  font-size: 28rpx;
  display: inline-block;
  white-space: nowrap;
  animation: horseRunning 15s linear infinite;
}

@keyframes horseRunning {
  from {
    transform: translateX(100%);
  }
  to {
    transform: translateX(-100%);
  }
}
